    "Node",
    "MySQL"
  ],
  "author": "German Mariles",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MarilesBeard5/express-react-template/issues"
  },
  "homepage": "https://github.com/MarilesBeard5/express-react-template#readme",
  "dependencies": {
    "concurrently": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}